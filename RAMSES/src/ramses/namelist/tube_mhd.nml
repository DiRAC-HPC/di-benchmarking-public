This namelist contains various input parameters for RAMSES runs

&RUN_PARAMS
hydro=.true.
nremap=0
ncontrol=10000
nsubcycle=10*1,
/

&AMR_PARAMS 
levelmin=5
levelmax=20
ngridmax=10000
nexpand=1
boxlen=2.5
/

&BOUNDARY_PARAMS
nboundary=2
ibound_min=-1,+1
ibound_max=-1,+1
bound_type= 2, 2
/

&INIT_PARAMS
nregion=2
region_type(1)='square'
region_type(2)='square'
x_center=0.5,1.75
length_x=1.0,1.5
d_region=1.0,0.2
u_region=0.0,0.0
v_region=0.0,0.0
w_region=0.0,0.0
p_region=1.0,0.2
A_region=1.0,1.0
B_region=1.0,-0.9899250
C_region=0.0,0.14112001
/

&OUTPUT_PARAMS
noutput=1
tout=0.4
/

&HYDRO_PARAMS
gamma=1.6666667
courant_factor=0.8
slope_type=2
riemann='roe'
/

&REFINE_PARAMS 
err_grad_d=0.01
err_grad_u=0.05
err_grad_p=0.05
interpol_var=0
interpol_type=2
/



